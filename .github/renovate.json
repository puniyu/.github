{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "helpers:pinGitHubActionDigests",
    ":semanticCommits"
  ],
  "rangeStrategy": "bump",
  "timezone": "Asia/Shanghai",
  "schedule": [
    "at any time"
  ],
  "packageRules": [
    {
      "groupName": "github actions",
      "matchManagers": [
        "github-actions"
      ],
      "schedule": [
        "at any time"
      ],
      "labels": [
        "dependencies"
      ]
    }
  ]
}